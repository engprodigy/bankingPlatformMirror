@model TheCoreBanking.Retail.Data.Models.TblTellerlogin



<div class="card ">

    @*<div class="col-md-2  pull-right "><h4 class=""> <small class="description">Retail Operation</small></h4></div>*@
    <div class="card-header ">
        <h4 class="card-title">Teller Posting - <small class="description">Tills on use</small></h4>
    </div>

    <div class="card-body ">

        <div id="tellerlogin-toolbar">
                <h7>This Till has been assigned for operation</h7>
            </div>

        <table id="tellerLoginTable" data-toggle="table"
               data-url="@Url.Action("listTellerLogin", "TellerAndTill")">
            <thead>
                <tr>
                    <th data-field="ledgername" data-sortable="true">Till Account Name</th>
                    <th data-field="accountid">Till Account Number</th>

                    <th data-events="tellerLogoutEvents" data-formatter="logoutFormatter"
                        data-align="center" data-width="5%">Logout</th>


                </tr>
            </thead>
        </table>


        <hr />

        <div class="row">
            <div class="col-md-4">
                <ul class="nav nav-pills nav-pills-primary flex-column" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active" data-toggle="tab" href="#link4" role="tablist">
                            Withdrawal
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="tab" href="#link5" role="tablist">
                            Transfer
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="tab" href="#link6" role="tablist">
                            Lodgement
                        </a>
                    </li>
                </ul>
            </div>
            <div class="col-md-8">
                <div class="tab-content">
                    <div class="tab-pane active" id="link4">
                        Collaboratively administrate empowered markets via plug-and-play networks. Dynamically procrastinate B2C users after installed base benefits.


                        <br><br>
                        Dramatically visualize customer directed convergence without revolutionary ROI.
                        <br><br>
                        <button class="btn btn-danger" id="btnTellerLogout" onclick="logOutTellerUser()">
                            <i class="now-ui-icons ui-1_simple-add"></i>
                            Click to Perform Teller Operation
                        </button>

                    </div>
                    <div class="tab-pane" id="link5">
                        Efficiently unleash cross-media information without cross-media value. Quickly maximize timely deliverables for real-time schemas.
                        <br><br>Dramatically maintain clicks-and-mortar solutions without functional solutions.
                    </div>
                    <div class="tab-pane" id="link6">
                        Completely synergize resource taxing relationships via premier niche markets. Professionally cultivate one-to-one customer service with robust ideas.
                        <br><br>Dynamically innovate resource-leveling customer service for state of the art customer service.
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts {

<environment include="Development">
    <script src="~/js/modules/TellerPosting.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/modules/TellerPosting.min.js"></script>
</environment>

}